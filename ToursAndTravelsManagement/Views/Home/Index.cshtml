<!-- CONTACT -->
<section class="section-padding bg-light" id="contact">
    <div class="container">
        <div class="row">
            <div class="col-12 text-center" data-aos="fade-down" data-aos-delay="150">
                <div class="section-title">
                    <h1 class="display-4 text-white fw-semibold">Gete in touch</h1>
                    <div class="line bg-warning bg-white"></div>
                    <p class="text-white">We love to craft unforgettable travel experiences for adventurers rather than generic tours and mundane itineraries. Let’s explore the world with passion and creativity!</p>
                </div>
            </div>
        </div>
        <div class="row justify-content-center" data-aos="fade-down" data-aos-delay="250">
            <div class="col-lg-8">
                <form asp-action="SubmitForm" method="post" class="row g-3 p-lg-5 p-4 bg-white theme-shadow">
                    <div class="form-group col-lg-6">
                        <input asp-for="FirstName" class="form-control" placeholder="Enter first name" required />
                        <span asp-validation-for="FirstName" class="text-danger"></span>
                    </div>
                    <div class="form-group col-lg-6">
                        <input asp-for="LastName" class="form-control" placeholder="Enter last name" required />
                        <span asp-validation-for="LastName" class="text-danger"></span>
                    </div>
                    <div class="form-group col-lg-12">
                        <input asp-for="Email" type="email" class="form-control" placeholder="Enter Email address" required />
                        <span asp-validation-for="Email" class="text-danger"></span>
                    </div>
                    <div class="form-group col-lg-12">
                        <input asp-for="Subject" class="form-control" placeholder="Enter subject" required />
                        <span asp-validation-for="Subject" class="text-danger"></span>
                    </div>
                    <div class="form-group col-lg-12">
                        <textarea asp-for="Message" rows="5" class="form-control" placeholder="Enter Message" required></textarea>
                        <span asp-validation-for="Message" class="text-danger"></span>
                    </div>
                    <div class="form-group col-lg-12 d-grid">
                        <button type="submit" class="btn btn-warning">Send Message</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>

