@model ToursAndTravelsManagement.Services.EmailService.ContactFormModel
@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_AnotherLayout.cshtml";
}

<!-- HERO -->
<section id="hero" class="min-vh-100 d-flex align-items-center text-center">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <h1 data-aos="fade-left" class="text-uppercase text-white fw-semibold display-3">Welcome to our
                    <span class="fw-bolder">Tours and Travels Management System</span>
                </h1>
                <h5 class="text-white mt-3 mb-4" data-aos="fade-right">We specialize in crafting unforgettable travel experiences that cater to your unique interests and preferences. With a passion for travel and a commitment to exceptional service, we are your trusted partners in exploring the globe.</h5>
                <div data-aos="fade-up" data-aos-delay="50">
                    <a class="btn btn-warning me-2" asp-controller="UserBookings" asp-action="AvailableTours">Get Started</a>
                    <a href="#" class="btn btn-light ms-2">Our Portfolio</a>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- CAROUSEL -->
<div id="tourCarousel" class="carousel slide" data-bs-ride="carousel">
    <div class="carousel-indicators">
        <button type="button" data-bs-target="#tourCarousel" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#tourCarousel" data-bs-slide-to="1" aria-label="Slide 2"></button>
        <button type="button" data-bs-target="#tourCarousel" data-bs-slide-to="2" aria-label="Slide 3"></button>
        <button type="button" data-bs-target="#tourCarousel" data-bs-slide-to="3" aria-label="Slide 4"></button>
        <button type="button" data-bs-target="#tourCarousel" data-bs-slide-to="4" aria-label="Slide 5"></button>
        <button type="button" data-bs-target="#tourCarousel" data-bs-slide-to="5" aria-label="Slide 6"></button>
    </div>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/images/carousel (1).jpg" class="d-block w-100" alt="Historic City Tour">
            <div class="carousel-caption d-none d-md-block">
                <h5>Historic City Tour</h5>
                <p>Delve into the rich history and culture of our city, visiting iconic landmarks and learning about the stories that shaped its past.</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="~/images/carousel (2).jpg" class="d-block w-100" alt="Nature's Wonders">
            <div class="carousel-caption d-none d-md-block">
                <h5>Nature's Wonders</h5>
                <p>Escape to the great outdoors and marvel at the beauty of our natural landscapes. From lush forests to stunning mountains, this tour offers a chance to connect with nature and enjoy its tranquility.</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="~/images/carousel (3).jpg" class="d-block w-100" alt="Culinary Adventure">
            <div class="carousel-caption d-none d-md-block">
                <h5>Culinary Adventure</h5>
                <p>Indulge in a gastronomic journey, exploring the local cuisine and sampling delicious dishes that reflect the region's culture and traditions.</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="~/images/carousel (1).jpg" class="d-block w-100" alt="Art and Culture Tour">
            <div class="carousel-caption d-none d-md-block">
                <h5>Art and Culture Tour</h5>
                <p>Immerse yourself in the local arts scene, visiting museums, galleries, and workshops to discover the talents of the area's creatives.</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="~/images/carousel (2).jpg" class="d-block w-100" alt="Adrenaline Rush">
            <div class="carousel-caption d-none d-md-block">
                <h5>Adrenaline Rush</h5>
                <p>For those seeking thrills, this tour offers a range of exciting activities, from white-water rafting to ziplining, ensuring an unforgettable and adrenaline-pumping experience.</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="~/images/carousel (3).jpg" class="d-block w-100" alt="Relaxation Retreat">
            <div class="carousel-caption d-none d-md-block">
                <h5>Relaxation Retreat</h5>
                <p>Unwind and rejuvenate with our relaxation retreat tour, featuring spa treatments, yoga sessions, and opportunities to relax in beautiful, serene settings.</p>
            </div>
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#tourCarousel" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#tourCarousel" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>

<!-- ABOUT -->
<section id="about" class="section-padding">
    <div class="container">
        <div class="row">
            <div class="col-12 text-center" data-aos="fade-down" data-aos-delay="50">
